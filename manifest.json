{
  "name": "My Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "https://*/*"
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabCapture"
  ],
  "background": {
    "service_worker": "backgroud.js"
  },
  "action": {
    "default_icon": "img/icon16.png",
    "default_title": "My Chrome Extension",
    "default_popup": "form.html"
  }
}








